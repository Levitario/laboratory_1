def check_key_in_values(d):
    for key, potential_keys in d.items():
        if key in potential_keys:  # если ключ присутствует в своем списке значений
            print(f"Ключ '{key}' присутствует в своем списке значений.")

        for potential_key in potential_keys:
            if potential_key in d and key not in d[potential_key]:
                print(f"Ключ '{key}' не присутствует в списке значений для ключа '{potential_key}'")


